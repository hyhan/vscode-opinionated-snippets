{
  "<script setup lang=\"ts\">": {
    "prefix": "sslt",
    "body": ["<script setup lang=\"ts\">", "$0", "</script>"],
    "description": "Script tag for setup & typescript\n"
  },
  "<style scoped lang=\"scss\">": {
    "prefix": "ssls",
    "body": ["<style scoped lang=\"${1:scss}\">", "$0", "</style>"],
    "description": "Style tag for scoped & scss\n"
  },
	"Vue2 Single File Component": {
		"prefix": "vue2",
		"body": [
			"<template>",
			"\t<div>",
			"\t\t$1",
			"\t</div>",
			"</template>",
			"",
			"<script>",
			"export default {",
			"\tname: '${WORKSPACE_NAME/(.*)/${1:/pascalcase}/}${TM_FILENAME_BASE/(.*)/${1:/pascalcase}/}',",
			"",
			"\tdata() {",
			"\t\treturn {",
			"\t\t\t${6}",
			"\t\t};",
			"\t},$0",
			"",
			"\tcreate() {",
			"\t\t${7}",
			"\t},",
			"",
			"\tmethods: {",
			"\t\t${8}",
			"\t},",
			"};",
			"</script>",
			""
		],
		"description": "Vue2 Single File Component with SCSS.\n"
	},
